{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": [
      "standard",
      "eslint:all"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "no-undef": 0,
      "sort-imports": 0,
      "no-magic-numbers": 0,
      "id-length": 0,
      "max-lines": 0,
      "max-lines-per-function": 0,
      "max-len": 0,
      "implicit-arrow-linebreak": 0,
      "sort-keys": 0,
      "max-statements": 0,
      "max-params": 0,
      "no-console": 1,
      "require-unicode-regexp": 0,
      "prefer-named-capture-group": 0,
      "no-ternary": 0,
      "multiline-ternary": 0,
      "no-warning-comments": 0,
      "function-call-argument-newline": 0,
      "consistent-return": 0,
      "no-promise-executor-return": 0,
      "no-async-promise-executor": 0,
      "require-await": 0,

      "function-paren-newline": ["error", "never"]
  }
}